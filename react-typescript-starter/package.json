{
  "name": "react-starter-typescript",
  "version": "0.0.1",
  "description": "Starter application with react, typescript, babel, webpack, jest",
  "main": "src/index.js",
  "scripts": {
    "compile": "tsc --build tsconfig.json && eslint --config .eslintrc.js --fix 'src/**/*.ts?(x)'",
    "pretest": "eslint --config .eslintrc.js --fix 'test/**/*.ts?(x)'",
    "test": "jest -c jest.config.js",
    "prestart": "npm run compile",
    "start": "webpack serve --hot --mode development --host 0.0.0.0",
    "prebuild": "npm run compile",
    "build": "webpack --mode production --env appVersion=$npm_package_version",
    "reset": "rm -rf dist node_modules"
  },
  "repository": {
    "type": "git",
    "url": "changeme"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^29.0.0",
    "@types/sinon": "^10.0.2",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^29.0.1",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.23.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^29.0.1",
    "sinon": "^14.0.0",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.0.0",
    "ts-jest": "^28.0.8",
    "typescript": "^4.3.5",
    "webpack": "^5.42.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.10.1"
  },
  "dependencies": {
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
