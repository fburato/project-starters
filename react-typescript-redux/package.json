{
  "name": "react-starter-typescript",
  "version": "0.0.1",
  "description": "Starter application with react, typescript, babel, webpack, jest",
  "main": "src/index.js",
  "scripts": {
    "compile": "tsc --build tsconfig.json && eslint --config .eslintrc.js --fix 'src/**/*.ts?(x)'",
    "pretest": "eslint --config .eslintrc.js --fix 'test/**/*.ts?(x)'",
    "test": "jest -c jest.config.js",
    "prestart": "npm run compile",
    "start": "webpack serve --hot --mode development --host 0.0.0.0",
    "prebuild": "npm run compile",
    "build": "webpack --mode production --env appVersion=$npm_package_version",
    "reset": "rm -rf dist node_modules"
  },
  "repository": {
    "type": "git",
    "url": "changeme"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.24",
    "@types/redux-mock-store": "^1.0.3",
    "@types/sinon": "^10.0.2",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.6",
    "eslint": "^7.30.0",
    "eslint-plugin-jsdoc": "^35.4.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "redux-mock-store": "^1.5.4",
    "sinon": "^11.1.1",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.0.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5",
    "webpack": "^5.42.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux-thunk": "^2.3.0"
  }
}
